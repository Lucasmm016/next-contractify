{
  "name": "next-contractify",
  "version": "1.0.0",
  "description": "Type-safe contract-based API client with Zod and code generation",
  "license": "MIT",
  "author": "Lucas Moreira de Matos",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucasmm016/next-contractify.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    }
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "next": "^16.1.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "zod": "^4.3.4"
  },
  "peerDependencies": {
    "next": ">=15"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "api",
    "zod",
    "type-safe",
    "contracts",
    "validation"
  ]
}